<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="materialtest.vivz.slidenerd.activities.ActivitySharedB">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Predicates"
        android:id="@+id/button6"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="predicatesClick" />

    <com.desu.experiments.view.widget.logView.LogView
        android:id="@+id/log_list"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_below="@+id/button6" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lazy Stream"
        android:id="@+id/button7"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/button6"
        android:layout_toEndOf="@+id/button6"
        android:onClick="LazyStreamClick" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="loan pattern"
        android:id="@+id/button8"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/button7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="loanPatternClick" />

</RelativeLayout>
